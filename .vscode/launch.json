{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug: service",
      "sourceMaps": true,
      "smartStep": true,
      "remoteRoot": "/usr/src/app",
      "localRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "/usr/src/app/src/*": "${workspaceFolder}/src/*"
      },
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "address": "0.0.0.0",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
 }
